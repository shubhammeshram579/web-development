<% include ./partial/header.ejs %>
<!-- css links  -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/locomotive-scroll@3.5.4/dist/locomotive-scroll.css">
    <link rel="stylesheet" href="/stylesheets/shop.css">


    <div class="curcer"></div>
    <div class="main">
      <!-- lending shop home page1 -->
        <div class="page1">
            <div class="card1">
                <h1>GOOD</h1>
                <h1>THINGS <i class="fa-solid fa-chevron-down"></i></h1>

            </div>
            <div class="card2">
                <p>We create high quality, sustainable, luxurious products - toiletries, apparel, blankets, candles.
                    Basically, things that feel like home. The best part? With every single purchase, you have the
                    potential to change the course of someone’s life.</p>

            </div>
        </div>

        <!-- product list pages2 -->
        <div class="page2 chaids">
                <% if (productList.length > 0) { %>
                    <% productList.slice(0,2).forEach(function(product) { %>
                      <div class="card1 chaid">
                        <a href="/product/<%= product._id %>">
                          <img src="<%= product.pImage %>" alt="">
                        </a>
                        <h5><%= product.producttitle %></h5>
                        <h6>$<%= product.pPrice %></h6>
                      </div>
                    <% }) %>
                  <% } else { %>
                    <h3>Not found</h3>
                  <% } %>
    


        </div>
        <div class="page3 chaids" >
            <% if (productList.length > 0) { %>
                <% productList.slice(2,5).forEach(function(product) { %>
                  <div class="card1 chaid">
                    <a href="/product/<%= product._id %>">
                      <img src="<%= product.pImage %>" alt="">
                    </a>
                    <h5><%= product.producttitle %></h5>
                    <h6>$<%= product.pPrice %></h6>
                  </div>
                <% }) %>
              <% } else { %>
                <h3>Not found</h3>
              <% } %>
        </div>

        <div class="page4 chaids">
            <% if (productList.length > 0) { %>
                <% productList.slice(5,9).forEach(function(product) { %>
                  <div class="card1 chaid">
                    <a href="/product/<%= product._id %>">
                      <img src="<%= product.pImage %>" alt="">
                    </a>
                    <h5 class="nameText"><%= product.producttitle %></h5>
                    <h6>$<%= product.pPrice %></h6>
                  </div>
                <% }) %>
              <% } else { %>
                <h3>Not found</h3>
              <% } %>
        </div>

        <!-- footer page 3 -->
        <div class="page5">
            <div class="endpage">
                <h1 class="enterText">ENTER YOUR EMAIL ADDRESS FOR GOOD</h1>
                <i class="ri-arrow-right-line"></i>
            </div>
        </div>
